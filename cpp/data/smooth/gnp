
file1 = "data/smooth/smooth.dat"

set grid
set key right top


# explicit analytical
plot file1 u 1 title 'Analytic: explicit' w l

# implicit analytical
replot file1 u 2 title 'Analytic: implicit' w l

# explicit-implicit analytical
replot file1 u 3 title 'Analytic: combined' w l


# explicit numerical
replot file1 u 4 title 'Numeric: explicit'

# implicit numerical
replot file1 u 5 title 'Numeric: implicit'

# explicit-implicit numerical
replot file1 u 6 title 'Numeric: exp-imp'

# implicit-explicit numerical
replot file1 u 7 title 'Numeric: imp-exp' pt 1

